{
    "name": "senses/laravel-gdpr",
    "description": "Laravel package for under the hood management of GDPR permission cookies.",
    "type": "library",
    "version": "1.0.0-rc1",
    "license": "MIT",
    "keywords": [
        "senses",
        "cookie",
        "gdpr",
        "comply",
        "eu"
	],
    "autoload": {
        "psr-4": {
            "Senses\\Gdpr\\": "src"
        }
    },
    "authors": [
        {
            "name": "Harry",
            "email": "harry@senses.co.uk",
            "homepage": "https://senses.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/view": "^7.0|^8.0|^9.0",
        "illuminate/cookie": "^7.0|^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Senses\\Gdpr\\GdprServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
